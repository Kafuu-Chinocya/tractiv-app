{
  "pages": [
    {
      "path": "pages/start-page/start-page",
      "style": {
        "transparentTitle": "always"
      }
    },
    {
      "path": "pages/login-page/login-page",
      "style": {
        "transparentTitle": "always"
      }
    },
    {
      "path": "pages/app-tutorial-page/app-tutorial-page",
      "style": {
        "transparentTitle": "always"
      }
    },
    {
      "path": "pages/main/main",
      "style": {
        "transparentTitle": "always"
      }
    }
